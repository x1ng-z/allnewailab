#import start
#here 'a1' and 'b1' are inupt pinname
#and 'c1' is output pin name
### customer code start
def main(input_data, context):
    a1=input_data['a1']['value']
    b1=input_data['b1']['value']
    c1=a1+b1
    #IN1 = input_data["IN1"] or None
    if 'c1' not in context:
        context['c1']=c1
    d1=a1+context['c1']
    context['c1']=d1
    OUT1 = {
        'c1':{
        	'value':d1
        }
    }
    return OUT1
### customer code end